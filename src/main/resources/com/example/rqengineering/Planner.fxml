<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="667.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rqengineering.Planner">
   <children>
      <Label alignment="CENTER" layoutX="1.0" layoutY="42.0" prefHeight="17.0" prefWidth="375.0" text="All outstanding jobs" />
      <HBox layoutY="59.0" prefHeight="144.0" prefWidth="375.0">
         <children>
            <TableView fx:id="jobsTableView" prefHeight="144.0" prefWidth="377.0">
              <columns>
                <TableColumn fx:id="jobIdColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="jobNameColumn" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="jobIsActiveColumn" prefWidth="75.0" text="Is Active" />
                  <TableColumn fx:id="jobDescriptionColumn" prefWidth="149.0" text="Description" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="290.0" prefHeight="17.0" prefWidth="375.0" text="Installation teams" />
      <HBox layoutX="1.0" layoutY="307.0" prefHeight="125.0" prefWidth="376.0">
         <children>
            <TableView fx:id="installationTeamsTableView" prefHeight="125.0" prefWidth="379.0">
              <columns>
                <TableColumn fx:id="installationTeamsIdColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="installationTeamsNameColumn" prefWidth="123.0" text="Name" />
                  <TableColumn fx:id="installationTeamsPeopleAmountColumn" prefWidth="99.0" text="People Amount" />
                  <TableColumn fx:id="installationTeamsIsActiveColumn" prefWidth="78.0" text="Active Job" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="138.0" layoutY="446.0" prefHeight="200.0" prefWidth="100.0" spacing="10">
         <children>
            <ComboBox fx:id="jobComboBox" prefWidth="150.0" />
            <ComboBox fx:id="teamComboBox" prefWidth="150.0" />
            <Button fx:id="setNewJob" alignment="CENTER" mnemonicParsing="false" text="Link A Job" textAlignment="CENTER" onAction="#linkJob" />
         </children>
      </VBox>
   </children>
</AnchorPane>
